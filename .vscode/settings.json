{
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.trimAutoWhitespace": true,
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "eslint.validate": [
        "typescript",
        "typescriptreact"
    ],
    "eslint.run": "onSave",
    "[scss]": {
      "editor.codeActionsOnSave": {
        "source.fixAll.stylelint": true
      }
    },
    "stylelint.validate": [
      "scss"
    ],
    "stylelint.snippet": [
      "scss"
    ]